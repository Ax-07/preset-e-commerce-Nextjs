{
  "name": "preset-ecommerce-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:merge": "prisma-merge -b prisma/schema/base.prisma -s prisma/schema/*.prisma -e prisma/schema.prisma -o prisma/schema.prisma",
    "prisma:generate": "npm run prisma:merge && prisma generate",
    "prisma:format": "npm run prisma:merge && prisma format",
    "prisma:lint": "npm run prisma:merge && prisma lint",
    "prisma:push": "npm run prisma:merge && prisma db push",
    "prisma:reset": "npm run prisma:merge && prisma migrate reset --force",
    "prisma:seed": "npm run prisma:merge && prisma db seed",
    "prisma:dev": "npm run prisma:merge && prisma migrate dev",
    "prisma:studio": "npm run prisma:merge && prisma studio",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
